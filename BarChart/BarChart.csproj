<Project Sdk="Microsoft.NET.Sdk.Razor">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <AddRazorSupportForMvc>true</AddRazorSupportForMvc>
    <Title>OkeahChart</Title>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Authors>Oceanedqn57</Authors>
    <Description>Un composant écrit en MudBlazor afin de réaliser des Diagrammes</Description>
    <PackageId>OkeahChart</PackageId>
    <Product>OkeahChart</Product>
  </PropertyGroup>

  <ItemGroup>
    <FrameworkReference Include="Microsoft.AspNetCore.App" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="MudBlazor" Version="6.0.17" />
  </ItemGroup>



</Project>
